/* pages/confirmOrder/confirmOrder.wxss */

.order-address-box {
  width: 100%;
  margin-bottom: 18rpx;
  background-color: #fff;
  font-size: 24rpx;
  color: #666;
}

.order-address-box-item-left {
  flex: 1;
}

.order-address-box-item-center {
  flex: 8;
}

.order-address-box-item-right {
  flex: 1;
}

.icon {
  margin-right: 5rpx;
  height: 42rpx;
  width: 42rpx;
  background: url(data:img/jpg;base64,)no-repeat 50% 50%;
  background-size: 32rpx 40rpx;
}

.nav {
  width: 13rpx;
  height: 24rpx;
  background: url(data:img/jpg;base64,) 50% 50% no-repeat;
  background-size: 100%;
}

.center {
  align-items: center;
  display: flex;
  justify-content: center;
}

.bottom-img {
  width: 100%;
  height: 9rpx;
  background: url(data:image/jpg;base64,) 50% 50% no-repeat;
  background-size: 100% 100%;
}

.order-info {
  width: 100%;
  background-color: #fff;
  font-size: 24rpx;
  margin-bottom: 18rpx;
}

.padding_10-0 {
  padding: 10rpx 0;
}

.bb {
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
}
.bottom_bar {
  width: 100%;
  height: 90rpx;
  background: #fff;
  line-height: 90rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 8;
}

.bottom-left {
  width: 70%;
  height: 100%;
  float: left;
  border-top: 2rpx solid rgba(0, 0, 0, 0.1);
  font-size: 28rpx;
}

.bottom-right {
  width: 30%;
  height: 100%;
  float: left;
  font-size: 28rpx;
  text-align: center;
}

.qhj-text {
  float: left;
  color: red;
  font-size: 32rpx;
  position: relative;
  margin-left: 10rpx;
}

.yj-text {
  float: left;
  margin-left: 20rpx;
  color: #888;
  margin-top: 4rpx;
}

.coupon-btn {
  color: white;
  background: linear-gradient(to left,  #ff5000,#ff8e00);
}

.point-btn {
  color: white;
  background: linear-gradient(to left, #ff9501, #ffcb01);
}

.icon-box {
  display: flex;
  height: 100%;
}

.icon-item {
  flex: 1;
}

.icon-image {
  width: 36rpx;
  height: 45rpx;
  margin: 0 auto;
  line-height: 45rpx;
}

.icon-text {
  font-size: 24rpx;
  text-align: center;
  height: 45rpx;
  line-height: 45rpx;
  color: #000;
}

button {
  padding-left: 0;
  padding-right: 0;
}

button[plain] {
  border: 0;
}

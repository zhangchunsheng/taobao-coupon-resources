/**app.wxss**/
@import 'style/weui.wxss';
@import 'style/font.wxss';
@import 'style/zanui.wxss';
.page{
  background: #eeeeee;
  font-size: 28rpx;
  color: #676767;
}
.container {
  overflow: hidden;
  box-sizing: border-box;
  margin-top: 10rpx;
}

.main-color {
  background-color: #ff93a5;
}

.box-ptop {
  padding-top: 10rpx;
}
.box {
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background-color: #fff;
}
.btn{
  width: 100%;
  height: 29px;
  line-height: 29px;
  text-align: center;
  padding:0 10rpx;
  background-color: #ff93a5;
  border-radius: 50rpx;
  color: #fff;
}
.point {
  margin-right: 5rpx;
  height: 50rpx;
  width: 50rpx;
  background: url(data:image/png;base64,)no-repeat 50% 50%;
  background-size: 50rpx 50rpx;
  margin-bottom: 12rpx;
}